{
  "name": "tree-sitter-fish",
  "version": "0.0.0",
  "description": "Fish tree-sitter grammar",
  "main": "bindings/node",
  "scripts": {
    "build": "tree-sitter generate",
    "build:wasm": "npm run build && tree-sitter build-wasm",
    "parse": "npm run build && tree-sitter parse",
    "start": "npm run build && tree-sitter web-ui",
    "test": "npm run build && tree-sitter test",
    "test:watch": "nodemon --ext js,txt --watch grammar.js --watch corpus  --exec npm run test"
  },
  "keywords": [
    "tree-sitter",
    "fish",
    "shell",
    "parser"
  ],
  "author": "Krnik",
  "license": "ISC",
  "dependencies": {
    "nan": "^2.14.2"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.19.4",
    "nodemon": "^2.0.7"
  }
}
